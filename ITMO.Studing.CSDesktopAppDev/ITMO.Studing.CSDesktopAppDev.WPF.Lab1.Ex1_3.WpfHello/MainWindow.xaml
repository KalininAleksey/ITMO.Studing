<Window x:Class="ITMO.Studing.CSDesktopAppDev.WPF.Lab1.Ex1.WpfHello.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITMO.Studing.CSDesktopAppDev.WPF.Lab1.Ex1.WpfHello"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="button" Content="Set Name" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click" Canvas.Top="57" Canvas.Left="10"/>
        <Button x:Name="button1" Content="Ret Name" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click_1" Canvas.Left="10" Canvas.Top="101"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="20" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="textBox_TextChanged" Canvas.Left="90" Canvas.Top="57"/>
        <Label x:Name="label" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="90" Canvas.Top="95"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75" Click="button3_Click"/>
        <TextBox Width="170" Canvas.Left="90" Canvas.Top="12" Name="myTextBox" />
    </Canvas>
</Window>
